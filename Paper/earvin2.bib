	Keyword = {Earth and Environmental Science},
	Note = {10.1007/s10950-009-9161-3},
	Pages = {9-26},
	Publisher = {Springer Netherlands},
	Title = {Probability gains of an epidemic-type aftershock sequence model in retrospective forecasting of M $\ge$ 5 earthquakes in Italy},
