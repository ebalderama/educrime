	Journal = {Bulletin of the Seismological Society of America},
	Number = {5},
	Pages = {2323-2339},
	Title = {Comparison of ETAS Parameter Estimates across Different Global Tectonic Zones},
	Url = {http://www.bssaonline.org/content/101/5/2323.abstract},
